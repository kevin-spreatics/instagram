{
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: g++.exe 활성 파일 빌드",
      "command": "g++",
      "args": [
        "-fdiagnostics-color=always",
        "-g",
        "-std=c++17",
        "${file}",
        "-o",
        "${fileDirname}\\${fileBasenameNoExtension}.exe",
        "-lws2_32"
      ],

      "options": {
        "cwd": "${fileDirname}",
        "env": {
    "TEMP": "C:\\Temp",
    "TMP": "C:\\Temp"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "디버거에서 생성된 작업입니다."
    }
  ],
  "version": "2.0.0"
}
